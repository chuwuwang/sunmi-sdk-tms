                android:text="enablePoweLockScreen"
                android:textAllCaps="false" />

            <CheckBox
                android:id="@+id/cb_show_navigation_bar_back_button_show"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="show" />

            <Button
                android:id="@+id/bt_show_navigation_bar_back_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="showNavigationBarBackButton"
                android:text="showNavigationBarBackButton"
                android:textAllCaps="false" />

            <CheckBox
                android:id="@+id/cb_show_navigation_bar_home_button_show"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="show" />

            <Button
                android:id="@+id/bt_show_navigation_bar_home_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="showNavigationBarHomeButton"
                android:text="showNavigationBarHomeButton"
                android:textAllCaps="false" />

            <CheckBox
                android:id="@+id/cb_show_navigation_bar_recents_button_show"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="show" />

            <Button
                android:id="@+id/bt_show_navigation_bar_recents_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
