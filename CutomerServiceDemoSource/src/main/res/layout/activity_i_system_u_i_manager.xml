                android:id="@+id/bt_is_navigation_bar_enabled"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="isNavigationBarEnabled"
                android:text="isNavigationBarEnabled"
                android:textAllCaps="false" />

            <EditText
                android:id="@+id/et_is_navigation_key_enabled_type"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="type" />

            <Button
                android:id="@+id/bt_is_navigation_key_enabled"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
