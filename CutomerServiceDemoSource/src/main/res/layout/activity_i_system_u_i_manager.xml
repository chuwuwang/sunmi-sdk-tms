                android:hint="value" />

            <Button
                android:id="@+id/bt_set_settings_ui_install_wifi_certificate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="setSettingsUiInstallWifiCertificate"
                android:text="setSettingsUiInstallWifiCertificate"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/bt_get_settings_ui_install_wifi_certificate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="getSettingsUiInstallWifiCertificate"
