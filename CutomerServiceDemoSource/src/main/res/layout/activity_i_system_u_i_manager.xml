                android:layout_height="wrap_content"
                android:onClick="setAirplaneModeBarClickable"
                android:text="setAirplaneModeBarClickable"
                android:textAllCaps="false" />

            <CheckBox
                android:id="@+id/cb_set_screen_brightness_automatic"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="automatic" />

            <EditText
                android:id="@+id/et_set_screen_brightness_value"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="value" />

            <Button
                android:id="@+id/bt_set_screen_brightness"
