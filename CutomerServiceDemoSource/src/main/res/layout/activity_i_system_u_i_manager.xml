                android:id="@+id/bt_set_a_p_n_visible"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="setAPNVisible"
                android:text="setAPNVisible"
                android:textAllCaps="false" />

            <EditText
                android:id="@+id/et_set_navigation_bar_button_location_left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="left" />

            <EditText
                android:id="@+id/et_set_navigation_bar_button_location_center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="center" />

            <EditText
                android:id="@+id/et_set_navigation_bar_button_location_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="right" />

            <Button
                android:id="@+id/bt_set_navigation_bar_button_location"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="setNavigationBarButtonLocation"
                android:text="setNavigationBarButtonLocation"
                android:textAllCaps="false" />

            <EditText
                android:id="@+id/et_set_settings_ui_install_wifi_certificate_value"
                android:layout_width="match_parent"
