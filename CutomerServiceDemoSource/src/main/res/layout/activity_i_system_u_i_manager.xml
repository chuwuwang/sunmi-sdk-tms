                android:id="@+id/bt_show_navigation_bar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="showNavigationBar"
                android:text="showNavigationBar"
                android:textAllCaps="false" />

            <CheckBox
                android:id="@+id/cb_show_status_bar_show"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="show" />

            <Button
                android:id="@+id/bt_show_status_bar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="showStatusBar"
                android:text="showStatusBar"
                android:textAllCaps="false" />

            <CheckBox
                android:id="@+id/cb_enable_navigation_bar_back_button_enable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="enable" />

            <Button
                android:id="@+id/bt_enable_navigation_bar_back_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="enableNavigationBarBackButton"
                android:text="enableNavigationBarBackButton"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/bt_is_navigation_bar_back_button_enabled"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="isNavigationBarBackButtonEnabled"
                android:text="isNavigationBarBackButtonEnabled"
                android:textAllCaps="false" />

            <CheckBox
                android:id="@+id/cb_enable_navigation_bar_home_button_enable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="enable" />

            <Button
                android:id="@+id/bt_enable_navigation_bar_home_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="enableNavigationBarHomeButton"
                android:text="enableNavigationBarHomeButton"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/bt_is_navigation_bar_home_button_enabled"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="isNavigationBarHomeButtonEnabled"
                android:text="isNavigationBarHomeButtonEnabled"
                android:textAllCaps="false" />

            <CheckBox
                android:id="@+id/cb_enable_navigation_bar_recents_button_enable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="enable" />

            <Button
                android:id="@+id/bt_enable_navigation_bar_recents_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="enableNavigationBarRecentsButton"
                android:text="enableNavigationBarRecentsButton"
                android:textAllCaps="false" />

            <Button
