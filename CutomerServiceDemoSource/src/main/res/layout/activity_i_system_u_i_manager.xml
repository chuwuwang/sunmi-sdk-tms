                android:id="@+id/cb_enable_powe_lock_screen_flag"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="flag" />

            <Button
                android:id="@+id/bt_enable_powe_lock_screen"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
