<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.sunmi.tmsservice.autodemo.ITelephoneManagerActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="10dp">

            <EditText
                android:id="@+id/et_get_carrier_info_slot_index"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="slotIndex" />

            <Button
                android:id="@+id/bt_get_carrier_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="getCarrierInfo"
                android:text="getCarrierInfo"
                android:textAllCaps="false" />

            <EditText
                android:id="@+id/et_get_phone_number_by_slot_index_slot_index"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="slotIndex" />

            <Button
                android:id="@+id/bt_get_phone_number_by_slot_index"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="getPhoneNumberBySlotIndex"
                android:text="getPhoneNumberBySlotIndex"
                android:textAllCaps="false" />

            <EditText
                android:id="@+id/et_send_text_message_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="address" />

            <EditText
                android:id="@+id/et_send_text_message_body"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="body" />

            <Button
                android:id="@+id/bt_send_text_message"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="sendTextMessage"
                android:text="sendTextMessage"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/bt_query_messages"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="queryMessages"
                android:text="queryMessages"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/bt_query_call_records"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="queryCallRecords"
                android:text="queryCallRecords"
                android:textAllCaps="false" />

            <EditText
                android:id="@+id/et_phone_number"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="phone number" />

            <Button
                android:id="@+id/bt_operation_call_logs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="insertCallLogs"
                android:text="operationCallLogs - insertCallLogs"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/bt_operation_call_logs2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="deletedOneCallLogs"
                android:text="operationCallLogs - deletedOneCallLogs"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/bt_query_contacts"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="queryContacts"
                android:text="queryContacts"
                android:textAllCaps="false" />

            <EditText
                android:id="@+id/et_contact_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="contact name" />

            <EditText
                android:id="@+id/et_contact_number"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="contact number" />

            <Button
                android:id="@+id/bt_operation_contacts"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="operationContacts"
                android:text="operationContacts"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/bt_delete_all_contacts"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="deleteAllContacts"
                android:text="deleteAllContacts"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/bt_add_message"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="addMessage"
                android:text="addMessage"
                android:textAllCaps="false" />

            <EditText
                android:id="@+id/et_send_text_message_with_call_back_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="address" />

            <EditText
                android:id="@+id/et_send_text_message_with_call_back_body"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="body" />

            <Button
                android:id="@+id/bt_send_text_message_with_call_back"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="sendTextMessageWithCallBack"
                android:text="sendTextMessageWithCallBack"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/bt_get_sim_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="getSimListInfo"
                android:text="getSimListInfo"
                android:textAllCaps="false" />
        </LinearLayout>

    </ScrollView>

</LinearLayout>