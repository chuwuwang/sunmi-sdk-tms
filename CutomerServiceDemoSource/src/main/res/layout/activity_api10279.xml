<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sunmi.tmsservice.apidemo.API10279Activity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="getGmsFlag"
            android:text="Get gms flag" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="isInnerScannerSupported"
            android:text="is inner scanner supported" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="isInnerPrinterSupported"
            android:text="is inner printer supported" />

    </LinearLayout>

</ScrollView>